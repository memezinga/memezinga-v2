  "dependencies": {
    "next": "12.0.7",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "lockfile-lint": "4.6.2",
    "npm-check-updates": "12.0.5",
    "standard": "16.0.4"
  },
  "standard": {
    "env": [
      "jest",
      "browser"
    ]
  }
}
